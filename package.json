{
  "name": "linear-algebra",
  "version": "3.1.3",
  "description": "Efficient, high-performance linear algebra library",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel --ignore test.js --out-dir dist src",
    "lint": "eslint ./src",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "codecov": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hiddentao/linear-algebra.git"
  },
  "keywords": [
    "linear",
    "algebra",
    "math",
    "precision",
    "float",
    "numerical",
    "analysis",
    "regression",
    "machine",
    "learning"
  ],
  "author": "Ramesh Nair <ram@hiddentao.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiddentao/linear-algebra/issues"
  },
  "homepage": "https://github.com/hiddentao/linear-algebra",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest-assertions": "^0.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "codecov": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.2.1"
  },
  "jest": {
    "roots": [
      "./src/"
    ],
    "testMatch": [
      "**/?(*.)test.js"
    ],
    "collectCoverageFrom": [
      "**/*.js"
    ]
  }
}
